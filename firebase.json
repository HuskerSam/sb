{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "./public/",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "functions",
      "assets/babylon/*.*"
    ],
    "headers": [{
      "source": "index.html",
      "headers": [{
        "key": "Access-Control-Allow-Origin",
        "value": "*"
      }]
    }],
    "rewrites": [{
      "source": "/supernote{,/**}",
      "destination": "/index.html"
    },{
      "source": "/Supernote{,/**}",
      "destination": "/index.html"
    },{
      "source": "/InfoView{,/**}",
      "destination": "/index.html"
    },{
      "source": "/tTV{,/**}",
      "destination": "/index.html"
    },{
      "source": "/SuperNote.html",
      "destination": "/index.html"
    },{
      "source": "/sales.aspx",
      "destination": "/index.html"
    }]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
