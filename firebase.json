{
  "hosting": {
    "public": "./public/",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "functions",
      "assets/babylon/*.*"
    ],
    "headers": [{
      "source": "index.html",
      "headers": [{
        "key": "Access-Control-Allow-Origin",
        "value": "*"
      }]
    }]
  }
}
