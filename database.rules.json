{
  "rules": {
    ".read": false,
    ".write": false,
    "project": {
      ".read": "auth !== null",
      ".write": "auth !== null",
      "$projectId": {
        "block": {
          ".indexOn": ["title", "blockFlag"]
        },
        "blockchild": {
          ".indexOn": ["parentKey", "animationIndex", "childType", "childName"]
        },
        "frame": {
          ".indexOn": ["parentKey"]
        }
      }
    },
    "projectTitles": {
      ".read": "auth !== null",
      ".write": "auth !== null"
    },
    "usersprofile": {
      ".read": "auth !== null",
      ".write": "auth !== null"
    }
  }
}
